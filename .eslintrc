{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "jest/globals": true
  },
  "rules": {
    "semi": "off",
    "arrow-parens": "off",
    "comma-dangle": "off",
    "max-len": "off",
    "require-jsdoc": "off",
    "operator-linebreak": "off",
    "no-case-declarations": "off"
  },
  "extends": [
    "eslint:recommended",
    "google"
  ],
  "plugins": ["jest"]
}